FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 3.480e3;

boundaryField
{
    wall_nozz
    {
        type            zeroGradient;
    }
    /*
    wall_end
    {
        type            zeroGradient;
    }
    */
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value		uniform 3.480e3;
    }
    
}

